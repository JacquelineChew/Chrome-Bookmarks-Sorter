{
  "name": "Bookmarks Sort By Date",
  "description": "Most recent bookmarks appear on top",
  "version": "1.7",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "storage",
    "cookies"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "red_bookmark.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}